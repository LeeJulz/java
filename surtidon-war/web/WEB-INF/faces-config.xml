<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/Consultar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETAILS</from-outcome>
            <to-view-id>/Modificar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Modificar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>SAVED</from-outcome>
            <to-view-id>/Consultar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LIST</from-outcome>
            <to-view-id>/Consultar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Borrado</from-outcome>
            <to-view-id>/Consultar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Crear.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Guardado</from-outcome>
            <to-view-id>/Consultar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Atras</from-outcome>
            <to-view-id>/menu.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ConsultarUsuarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLES</from-outcome>
            <to-view-id>/ModificarUsuarios.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ModificarUsuarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>SALVADO</from-outcome>
            <to-view-id>/ConsultarUsuarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LISTA</from-outcome>
            <to-view-id>/ConsultarUsuarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>USUARIOBORRADO</from-outcome>
            <to-view-id>/ConsultarUsuarios.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
